{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "score-snap",
  "main": ".open-next/worker.js",
  "compatibility_date": "2026-02-10",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  }
}
